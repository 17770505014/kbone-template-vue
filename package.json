{
    "name": "kbone-template-vue",
    "description": "a simple vue template for kbone",
    "author": "wechat-miniprogram",
    "scripts": {
        "start": "npm run mp",
        "mp": "",
        "build-mp": "cross-env NODE_ENV=production webpack --config build/webpack.mp.config.js --progress --hide-modules",
        "web": "",
        "build-web": "rimraf dist/web && cross-env NODE_ENV=production webpack --config build/webpack.config.js --progress --hide-modules",
        "build": "npm run build-mp && npm run build-web"
    },
    "dependencies": {
        "vue": "^2.5.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "autoprefixer": "^9.6.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "less": "^3.7.1",
        "thread-loader": "^1.2.0",
        "less-loader": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^0.5.1",
        "url-loader": "^1.1.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.4",
        "html-webpack-plugin": "^4.0.0-beta.5",
        "mini-css-extract-plugin": "^0.5.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "stylehacks": "^4.0.3",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "mp-webpack-plugin": "latest",
        "vue-improve-loader": "latest",
        "reduce-loader": "latest"
    }
}